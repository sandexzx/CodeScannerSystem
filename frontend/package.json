{
  "name": "datamatrix-scanner",
  "version": "1.0.0",
  "private": true,
  "main": "dist/electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.electron.json && tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron --no-sandbox .\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.46",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.15.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^7.2.0"
  }
}
